private void TextBlock_MouseLeftButtonUp0(object sender, MouseButtonEventArgs e)
 {
 			p = null;
            if (p == null)
            {
                RunPath = AppDomain.CurrentDomain.BaseDirectory;
                string ip = AppDomain.CurrentDomain.BaseDirectory + "国核示范\\EOP\\SNG-GX-GJP-201 REV.A PRE 触发反应堆停堆或专设安全设施响应规程.doc";
                p = new System.Diagnostics.Process();
                p.StartInfo.FileName = ip;
                p.Start();
            }
            else
            {
                if (p.HasExited) //是否正在运行
                {
                    p.Start();
                }
            }
            p.StartInfo.WindowStyle = System.Diagnostics.ProcessWindowStyle.Normal;
 }  
     
